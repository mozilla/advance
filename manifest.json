{
  "applications": {
    "gecko": {
      "strict_min_version": "54.0a1",
      "id": "support@laserlike.com"
    }
  },
  "background": {
    "scripts": [
      "background/index.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/laserlike_ff_icon.png",
    "browser_style": true,
    "default_title": "Laserlike Read Next"
  },
  "sidebar_action": {
    "default_icon": "icons/laserlike_ff_icon.png",
    "default_title": "Laserlike",
    "default_panel": "sidebar/panel.html"
  },
  "description": "Shows related articles to the active page and For you feed based on your browsing history.",
  "homepage_url": "https://www.laserlike.com",
  "manifest_version": 2,
  "name": "Laserlike",
  "permissions": [
    "storage",
    "activeTab",
    "*://*.laserlike.com/",
    // "*://localhost/", // TO BE REMOVED BEFORE publishing
    "history",
    "cookies",
    "tabs",
    "menus"
  ],
  "version": "0.9.24"
}